package Day1;

public class Fact 
{
	static int calculateFactorial(int n)
	{
		if(n == 0)
			return 1;
		else
			return (n*calculateFactorial(n-1));
	}
	public static void main(String args[])
	{
		int fact = 1;
		int numb = 2;
		fact = calculateFactorial(numb);
		System.out.println("factorial of " +numb+ " is:"+fact);
	}
}



